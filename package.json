{
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "name": "kuba",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "formatter": "yarn workspace @kuba/scripts start formatter",
    "generate": "yarn workspace @kuba/scripts start",
    "serve": "yarn workspace @kuba/scripts start serve",
    "test": "yarn workspace @kuba/scripts start test"
  },
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "actions/*",
      "blocks/*",
      "buckets/*",
      "directives/*",
      "development/*",
      "elements/*",
      "filters/*",
      "infra/*",
      "models/*",
      "modules/*",
      "pages/*",
      "pixels/*",
      "standard/*"
    ]
  }
}
